CREATE TABLE SCORE(
    SNO NUMBER,
    NAME VARCHAR2(30),
    KOR VARCHAR2(30),
    ENG VARCHAR2(30),
    MATH VARCHAR2(30)
);

ALTER TABLE SCORE ADD CONSTRAINT SCORE_PK PRIMARY KEY (SNO);

CREATE SEQUENCE SCORE_SEQ NOCACHE;


/*********************************/

CREATE TABLE BOARD(
    BNO NUMBER,
    NAME VARCHAR2(30),
    TITLE VARCHAR2(30),
    CONTENT VARCHAR2(30)
);

ALTER TABLE BOARD ADD CONSTRAINT BOARD_PK PRIMARY KEY (BNO);

CREATE SEQUENCE BOARD_SEQ NOCACHE;

SELECT * FROM BOARD;